              - [A_Cercano, C_Barato, 3]
              - [A_Cercano, D_Centrico, 1/5]
              - [B_Reformado, C_Barato, 5]
              - [B_Reformado, D_Centrico, 1/3]
              - [C_Barato, D_Centrico, 1/7]
          children: *alternatives
    Calidad:
      preferences:
        pairwise:
          # Estado(1)vsTam
          - [Estado, Tamaño Hab., 1]
      children:
