          preferences:
            pairwise:
              # (A-B, A-C, A-D, B-C, B-D, C-D) = c(1/7, 1, 1/3, 7, 5, 1/3)
              - [A_Cercano, B_Reformado, 1/7]
              - [A_Cercano, C_Barato, 1]
              - [A_Cercano, D_Centrico, 1/3]
              - [B_Reformado, C_Barato, 7]
              - [B_Reformado, D_Centrico, 5]
              - [C_Barato, D_Centrico, 1/3]
