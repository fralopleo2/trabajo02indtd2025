        Tamaño Hab.:
          preferences:
            pairwise:
              # (A-B, A-C, A-D, B-C, B-D, C-D) = c(1/3, 1/5, 1, 1/3, 3, 5)
              - [A_Cercano, B_Reformado, 1/3]
              - [A_Cercano, C_Barato, 1/5]
              - [A_Cercano, D_Centrico, 1]
              - [B_Reformado, C_Barato, 1/3]
              - [B_Reformado, D_Centrico, 3]
              - [C_Barato, D_Centrico, 5]
          children: *alternatives
    Ambiente:
      preferences:
        pairwise:
          # Ruido(3)vsCompis
          - [Ruido, N.º Compis, 3]
      children:
        Ruido:
          preferences:
            pairwise:
              # (A-B, A-C, A-D, B-C, B-D, C-D) = c(1/5, 1/5, 1/7, 1, 1/3, 1/3)
              - [A_Cercano, B_Reformado, 1/5]
              - [A_Cercano, C_Barato, 1/5]
              - [A_Cercano, D_Centrico, 1/7]
              - [B_Reformado, C_Barato, 1]
              - [B_Reformado, D_Centrico, 1/3]
              - [C_Barato, D_Centrico, 1/3]
          children: *alternatives
        N.º Compis:
          preferences:
            pairwise:
              # (A-B, A-C, A-D, B-C, B-D, C-D) = c(1/3, 1/3, 1/3, 1, 1, 1)
              - [A_Cercano, B_Reformado, 1/3]
              - [A_Cercano, C_Barato, 1/3]
              - [A_Cercano, D_Centrico, 1/3]
              - [B_Reformado, C_Barato, 1]
              - [B_Reformado, D_Centrico, 1]
              - [C_Barato, D_Centrico, 1]
